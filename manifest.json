{
  "manifest_version": 2,
  "name": "Omnibox App Launcher",
  "description": "Launch app from Omnibox (type 'app')",
  "version": "1.0.0",
  "omnibox": { "keyword" : "app" },
  "permissions": [
    "management"
  ],
  "icons": {
    "16": "assets/icon_16.png",
    "32": "assets/icon_32.png",
    "48": "assets/icon_48.png",
    "256": "assets/icon_256.png",
    "512": "assets/icon_512.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  }
}